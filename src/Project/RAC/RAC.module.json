{
    "namespace": "Project.RAC",
	"references": [ "Foundation.*", "Feature.*" ],
    "items": {
      "includes": [
        {
          "name": "placeholders",
          "path": "/sitecore/layout/Placeholder Settings/Project/RAC",
          "allowedPushOperations": "CreateUpdateAndDelete"
        },
        {
          "name": "renderings",
          "path": "/sitecore/layout/Renderings/Project/RAC",
          "allowedPushOperations": "CreateUpdateAndDelete"
        },
        {
          "name": "templates",
          "path": "/sitecore/templates/Project/RAC",
          "allowedPushOperations": "CreateUpdateAndDelete"
        },
        {
          "name": "branches",
          "path": "/sitecore/templates/Branches/Project/RAC",
          "allowedPushOperations": "CreateUpdateAndDelete"
        },
        {
          "name": "media",
          "path": "/sitecore/Media Library/Project/RAC",
          "allowedPushOperations": "CreateUpdateAndDelete"
        },
        {
          "name": "apikey",
          "path": "/sitecore/system/Settings/Services/API Keys/rac",
          "allowedPushOperations": "CreateUpdateAndDelete"
        },
        {
          "name": "content",
          "path": "/sitecore/content/RAC",
          "allowedPushOperations": "CreateOnly",
